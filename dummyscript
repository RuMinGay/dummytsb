local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local Camera = game.Workspace.CurrentCamera

local function activateTrashSkill()
    local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")
    local TrashCans = workspace.Map.Trash:GetChildren()

    local OldCFrame = HumanoidRootPart.CFrame
    local excludeTrashCans = {}

    local OldCameraCFrame = Camera.CFrame

    Camera.CameraType = Enum.CameraType.Scriptable
    Camera.CFrame = CFrame.new(HumanoidRootPart.Position + Vector3.new(0, 5, -10), HumanoidRootPart.Position)

    local function findClosestTrashCan(exclude)
        local closest
        for _, trash in pairs(TrashCans) do
            local part = trash:FindFirstChildOfClass("MeshPart")
            if part and part.Transparency ~= 1 and not exclude[part] then
                if not closest or (part.Position - HumanoidRootPart.Position).Magnitude < (closest.Position - HumanoidRootPart.Position).Magnitude then
                    closest = part
                end
            end
        end
        return closest
    end

    while true do
        local can = findClosestTrashCan(excludeTrashCans)
        if not can then break end
        local parent = can.Parent
        local pos = can.Position - Vector3.new(0, 2, 0)

        HumanoidRootPart.CFrame = CFrame.new(pos)
        task.wait(0.1)

        HumanoidRootPart.CFrame = CFrame.new(HumanoidRootPart.Position, can.Position)

        while can and can.Parent do
            mouse1click()  
            task.wait(0.1) 
            mouse1click()  
            task.wait(0.2) 

            if can.Transparency == 1 or not parent:IsDescendantOf(workspace) then
                break
            end
        end

        if can.Transparency == 1 or not parent:IsDescendantOf(workspace) then
            break
        else
            excludeTrashCans[can] = true
        end
    end

    task.wait(0.5)

    Camera.CameraType = Enum.CameraType.Custom
    Camera.CFrame = OldCameraCFrame

    task.wait(0)
    HumanoidRootPart.CFrame = OldCFrame
end

local function createTrashTool()
    local tool = Instance.new("Tool")
    tool.RequiresHandle = false
    tool.Name = "쓰통(움직이기 쉬프트락 ㄴㄴ)"

    tool.Activated:Connect(function()
        tool:Destroy()

        task.spawn(activateTrashSkill)

        task.delay(1, createTrashTool)
    end)

    tool.Parent = LocalPlayer:WaitForChild("Backpack")
end

createTrashTool()

